public class Main {

    public static void main(String[] args) {
    int n = 137;
    boolean yo = isPrime(n);
    System.out.println(yo);

    }
    public static boolean isPrime(int n) {
        for (int i = 2; i < n; i++) {
            if (n % i  == 0){
                return false ;      //Has to be false because range is between 2 - n
            }
        } return true;
    }
}
